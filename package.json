{
  "name": "arena-badges",
  "version": "1.0.0",
  "description": "Arena Badges project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "mu:run": "ts-node src/project.ts run mu",
    "mu:run:once": "ts-node src/project.ts run mu --once",
    "mu:run:verbose": "ts-node src/project.ts run mu --verbose",
    "mu:run:dry-run": "ts-node src/project.ts run mu --dry-run",
    "mu:run:dry-run:once": "ts-node src/project.ts run mu --dry-run --once",
    "mu:run:dry-run:once:verbose": "ts-node src/project.ts run mu --dry-run --once --verbose",
    "mu:badge": "ts-node src/project.ts badge mu",
    "mu:badge:once": "ts-node src/project.ts badge mu --once",
    "mu:badge:verbose": "ts-node src/project.ts badge mu --verbose",
    "mu:badge:dry-run": "ts-node src/project.ts badge mu --dry-run",
    "mu:badge:dry-run:once": "ts-node src/project.ts badge mu --dry-run --once",
    "mu:badge:dry-run:once:verbose": "ts-node src/project.ts badge mu --dry-run --once --verbose",
    "mu:badge:dry-run:once:address": "ts-node src/project.ts badge mu --dry-run --once --export-addresses",
    "mu:leaderboard": "ts-node src/project.ts leaderboard mu",
    "mu:leaderboard:once": "ts-node src/project.ts leaderboard mu --once",
    "mu:leaderboard:verbose": "ts-node src/project.ts leaderboard mu --verbose",
    "mu:leaderboard:once:verbose": "ts-node src/project.ts leaderboard mu --once --verbose",
    "boi:badge": "ts-node src/project.ts badge boi",
    "boi:badge:once": "ts-node src/project.ts badge boi --once",
    "boi:badge:verbose": "ts-node src/project.ts badge boi --verbose",
    "boi:badge:dry-run": "ts-node src/project.ts badge boi --dry-run",
    "boi:badge:dry-run:once": "ts-node src/project.ts badge boi --dry-run --once",
    "boi:badge:dry-run:once:verbose": "ts-node src/project.ts badge boi --dry-run --once --verbose",
    "boi:badge:dry-run:once:address": "ts-node src/project.ts badge boi --dry-run --once --export-addresses",
    "test": "jest",
    "test:snowtrace-sorting": "ts-node tests/snowtraceOrderTest.ts",
    "test:arena-api": "ts-node tests/arenaApiTest.ts",
    "kill": "taskkill /F /IM node.exe"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.1",
    "alchemy-sdk": "^3.5.8",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^5.1.0",
    "moralis": "^2.27.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
